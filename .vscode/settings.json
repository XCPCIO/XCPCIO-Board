{
  "editor.tabSize": 4,
  "editor.insertSpaces": true,

  "prettier.enable": false,
  "editor.formatOnSave": false,

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },

  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml"
  ],

  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.associations": {
    "*.css": "postcss"
  },

  "typescript.tsdk": "node_modules/typescript/lib",
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[shellscript]": {
    "editor.formatOnSave": true
  },

  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": "locales",
  "i18n-ally.sortKeys": true,
  "cSpell.words": [
    "antd",
    "antfu",
    "Attributify",
    "buildx",
    "bumpp",
    "ccpc",
    "codeforces",
    "composables",
    "contlen",
    "dedupe",
    "Delivr",
    "demi",
    "DOCKERHUB",
    "esmo",
    "flowbite",
    "fontawesome",
    "fortawesome",
    "frontmatter",
    "fsxx",
    "gsap",
    "highcharts",
    "iconify",
    "icpc",
    "Immer",
    "immerable",
    "intlify",
    "metas",
    "nocheck",
    "noopener",
    "nums",
    "pageview",
    "papaparse",
    "pinia",
    "pnpm",
    "prettierrc",
    "shiki",
    "sizecheck",
    "stnd",
    "tailwindcss",
    "tanstack",
    "taze",
    "typecheck",
    "unhead",
    "unocss",
    "unplugin",
    "vite",
    "vitepress",
    "vitesse",
    "vueuse",
    "Webfont",
    "XCPC",
    "XCPCIO"
]
}
